<h2>Hackathons</h2>

<div class="hackathons">
  <% @hackathons.each do | hackathon | %>
  <div class="row">
    <ul>
      <li>
        <h2>
          <a href="/hackathons/<%= hackathon.id %>">
            Hackathon <%= hackathon.title %>
          </a>
        </h2>
      </li>
    </ul>
  </div>
  <% end %>
</div>
